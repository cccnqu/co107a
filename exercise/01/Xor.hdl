/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
     Not(in=a,out=c);
    Not(in=b,out=d);
    And(a=c,b=b,out=e);
    And(a=a,b=d,out=f);
    Or(a=e,b=f,out=out);
}
