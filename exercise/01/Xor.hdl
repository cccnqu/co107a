/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=aorb);

	Nand(a=a, b=b, out=anandb);

	And(a=aorb, b=anandb, out=out);
}